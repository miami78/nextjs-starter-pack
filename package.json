{
  "name": "nextjs-starter-pack",
  "private": true,
  "version": "1.0.0",
  "description": "A JavaScript starter for Next.js that includes all you need to build your next ðŸ¦„ project.",
  "author": {
    "name": "Larry Miami",
    "email": "miamielarry@gmail.com"
  },
  "keywords": [
    "nextjs",
    "starter",
    "unicorn",
    "airbnb-style-guides",
    "husky",
    "prettier",
    "eslint"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint **/*.js --report-unused-disable-directives",
    "lint-fix": "eslint --fix --ext .js,.jsx .",
    "format": "prettier --ignore-path .prettierignore --write .",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*js": [
      "yarn lint --fix",
      "yarn format"
    ]
  },
  "dependencies": {
    "next": "12.0.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "eslint": "^8.1.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-next": "12.0.2",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^7.0.0",
    "lint-staged": "^11.2.6",
    "prettier": "^2.4.1"
  }
